	"Project" -> "User" [arrowtail=crow, arrowhead=crow, dir=both color="#E7F3D5"]
	"User" -> "Mailboxer::Message" [label="messages", arrowtail=crow, arrowhead=crow, dir=both color="#1E6D20"]
	"User" -> "Mailboxer::Receipt" [label="receipts", arrowtail=crow, arrowhead=crow, dir=both color="#CD9721"]
	"User" -> "UserExam" [arrowtail=crow, arrowhead=crow, dir=both color="#96E4CF"]
