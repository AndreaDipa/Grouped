	"Exam" -> "ExamProject" [arrowtail=crow, arrowhead=crow, dir=both color="#E0F799"]
	"Exam" -> "Project" [arrowtail=crow, arrowhead=crow, dir=both color="#0B3470"]
	"ExamProject" -> "Project" [arrowtail=crow, arrowhead=crow, dir=both color="#0AAE9E"]
	"Project" -> "UserProject" [arrowtail=crow, arrowhead=crow, dir=both color="#D866B2"]
	"Project" -> "User" [arrowtail=crow, arrowhead=crow, dir=both color="#99C323"]
	"Project" -> "User" [arrowtail=crow, arrowhead=crow, dir=both color="#D2BF3C"]
	"User" -> "Mailboxer::Message" [label="messages", arrowtail=crow, arrowhead=crow, dir=both color="#FCDFFD"]
	"User" -> "Mailboxer::Receipt" [label="receipts", arrowtail=crow, arrowhead=crow, dir=both color="#0CC5B5"]
	"User" -> "UserExam" [arrowtail=crow, arrowhead=crow, dir=both color="#0CDC36"]
	"User" -> "UserProject" [arrowtail=crow, arrowhead=crow, dir=both color="#CC6920"]
