digraph models_diagram {
	graph[overlap=false, splines=true, bgcolor="none"]
	"ApplicationRecord" []
	"Exam" [shape=Mrecord, label="{Exam|id :integer\lname :varchar\lcode :varchar\lcreated_at :datetime(6)\lupdated_at :datetime(6)\ldescription :text\l}"]
	"ExamProject" [shape=Mrecord, label="{ExamProject|id :integer\lproject_id :integer\lexam_id :integer\lcreated_at :datetime(6)\lupdated_at :datetime(6)\l}"]
	"Project" [shape=Mrecord, label="{Project|id :integer\lproject_name :varchar\ldescription :varchar\lcreated_at :datetime(6)\lupdated_at :datetime(6)\lrepo :varchar\luser_id :integer\l}"]
	"User" [shape=Mrecord, label="{User|id :integer\lemail :varchar\lencrypted_password :varchar\lreset_password_token :varchar\lreset_password_sent_at :datetime\lremember_created_at :datetime\lfirst_name :varchar\llast_name :varchar\lroles_mask :integer\l}"]
	"UserExam" [shape=Mrecord, label="{UserExam|id :integer\luser_id :integer\lexam_id :integer\lcreated_at :datetime(6)\lupdated_at :datetime(6)\l}"]
	"UserProject" [shape=Mrecord, label="{UserProject|id :integer\lproject_id :integer\luser_id :integer\lcreated_at :datetime(6)\lupdated_at :datetime(6)\l}"]
